{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "ProctoringDbConnection": ""
  },
  "JwtInfo": {
    "keyString": ""
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "ProctoringLogs",
        "Args": {
          "connectionString": "",
          "tableName": "proctoring_api_logs",
          "storeTimestampInUtc": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Studi.Api.Proctoring.log",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Debug"
      }
    ]
  },
  "AllowedHosts": "*",
  "PrivateJwtKey": "",
  "AllowedCorsOrigins": [
    "http://localhost"
  ],

  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "ImageRepository": {
    "UserIDsRootDirectory": "",
    "ProctoringRootDirectory": ""
  },
  "FeatureFlipping": {
    "ActivateCheckImageIntervalElapsed": null
  }
}
